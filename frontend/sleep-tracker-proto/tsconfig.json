{
  "extends": "./.quasar/tsconfig.json"
}

{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"] // Adjust based on your project structure
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}